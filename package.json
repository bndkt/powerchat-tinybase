{
  "name": "powerchat",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "clear": "rm -f bun.lockb && find ./ -type d -and \\( -name node_modules -or -name build -or -name dist \\) -exec rm -rf '{}' +",
    "ncu": "npx npm-check-updates -p bun -ui",
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --clear",
    "doc": "npx expo-doctor@latest",
    "prebuild": "expo prebuild --platform ios --clean --no-install",
    "build:simulator": "bun x eas-cli@latest build --profile simulator --platform ios --local",
    "build:development": "bun x eas-cli@latest build --profile development --platform ios --local",
    "build:production": "bun x eas-cli@latest build --profile production --platform ios --local",
    "update:production": "bun x eas-cli@latest update --non-interactive -p ios --branch=production --auto",
    "types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "lint": "eslint src"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@faker-js/faker": "8.3.1",
    "@journeyapps/powersync-sdk-react-native": "1.0.0",
    "@journeyapps/react-native-quick-sqlite": "1.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@shopify/flash-list": "1.4.3",
    "@supabase/supabase-js": "2.39.0",
    "@tamagui/animations-react-native": "1.79.6",
    "@tamagui/babel-plugin": "1.79.6",
    "@tamagui/config": "1.79.6",
    "@tamagui/font-inter": "1.79.6",
    "@tamagui/lucide-icons": "1.79.6",
    "@tamagui/theme-base": "1.79.6",
    "base-64": "^1.0.0",
    "date-fns": "^2.30.0",
    "expo": "~49.0.21",
    "expo-crypto": "~12.4.1",
    "expo-dev-client": "~2.4.12",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.14",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-fetch-api": "^3.0.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-pager-view": "6.2.0",
    "react-native-polyfill-globals": "^3.1.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "0.19.9",
    "tamagui": "1.79.6",
    "text-encoding": "^0.7.0",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/plugin-transform-async-generator-functions": "^7.23.4",
    "@types/react": "18.2.42",
    "eslint": "8.55.0",
    "eslint-config-universe": "12.0.0",
    "prettier": "^3.1.0",
    "typescript": "5.3.2"
  },
  "license": "UNLICENSED",
  "private": true
}
