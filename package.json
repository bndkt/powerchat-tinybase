{
  "name": "powerchat",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "clear": "rm -f bun.lockb && find ./ -type d -and \\( -name node_modules -or -name build -or -name dist \\) -exec rm -rf '{}' +",
    "ncu": "npx npm-check-updates -p bun -ui -ws",
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --clear",
    "doc": "npx expo-doctor@latest",
    "prebuild": "expo prebuild --platform ios --clean",
    "build:simulator": "eas build --profile simulator --platform ios --local",
    "build:development": "eas build --profile development --platform ios --local",
    "build:production": "eas build --profile production --platform ios --local",
    "update:production": "eas update --non-interactive -p ios --branch=production --auto",
    "types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "lint": "eslint src"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "1.0.2",
    "@faker-js/faker": "8.2.0",
    "@journeyapps/powersync-sdk-react-native": "0.0.1",
    "@journeyapps/react-native-quick-sqlite": "0.0.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@shopify/flash-list": "1.6.1",
    "@supabase/supabase-js": "2.38.2",
    "@tamagui/animations-react-native": "1.74.21",
    "@tamagui/babel-plugin": "1.74.21",
    "@tamagui/config": "1.74.21",
    "@tamagui/font-inter": "1.74.21",
    "@tamagui/lucide-icons": "1.74.21",
    "@tamagui/theme-base": "1.74.21",
    "base-64": "^1.0.0",
    "date-fns": "^2.30.0",
    "expo": "50.0.0-alpha.6",
    "expo-application": "~5.6.0",
    "expo-asset": "~8.13.0",
    "expo-crypto": "~12.7.0",
    "expo-dev-client": "~3.2.0",
    "expo-device": "~5.8.0",
    "expo-file-system": "~15.8.0",
    "expo-font": "~11.8.0",
    "expo-linking": "~6.1.1",
    "expo-router": "3.1.2",
    "expo-splash-screen": "~0.24.0",
    "expo-status-bar": "~1.9.0",
    "expo-system-ui": "~2.7.0",
    "expo-updates": "~0.22.0",
    "expo-web-browser": "~12.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-fetch-api": "^3.0.0",
    "react-native-gesture-handler": "2.12.1",
    "react-native-get-random-values": "~1.8.0",
    "react-native-pager-view": "6.2.0",
    "react-native-polyfill-globals": "^3.1.0",
    "react-native-reanimated": "3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "0.19.7",
    "tamagui": "1.74.21",
    "text-encoding": "^0.7.0",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/plugin-transform-async-generator-functions": "7.23.2",
    "@types/react": "18.2.30",
    "eslint": "8.51.0",
    "eslint-config-universe": "12.0.0",
    "prettier": "^3.0.3",
    "typescript": "5.2.2"
  },
  "license": "UNLICENSED",
  "private": true
}
